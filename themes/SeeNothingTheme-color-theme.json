	"tokenColors": [
		{
			"name": "comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#666",
				"fontStyle": "italic"
			}
		},
		{
			"name": "keyword",
			"scope": [
				"keyword.control",
				"keyword",
				"punctuation.definition.keyword",
				"markup.bold",
			],
			"settings": {
				"foreground": "#3f8f3b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "function",
			"scope": [
				"meta.function-call.generic",
				"meta.function-call",
				"entity.name.function",
				"entity.name.tag",
				"heading"
			],
			"settings": {
				"foreground": "#8a0e0a",
				"fontStyle": ""
			}
		},
		{
			"name": "operators",
			"scope": [
				"keyword.operator",
				"meta.paragraph"
			],
			"settings": {
				"foreground": "#1a86a7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "brackets",
			"scope": [
				"punctuation.definition",
				"constant.character.format.placeholder",
				"punctuation.parenthesis",
				"meta.brace",
				"punctuation.section"
			],
			"settings": {
				"foreground": "#a27719",
				"fontStyle": ""
			}
		},
		{
			"name": "string",
			"scope": [
				"string",
				"string.quoted",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#bbb",
				"fontStyle": "italic"
			}
		},
		{
			"name": "storage",
			"scope": [
				"storage.type",
				"entity.other.attribute-name",
				"keyword.other.unit",
				"punctuation.definition.constant.css",
				"markup.italic"
			],
			"settings": {
				"foreground": "#377934",
				"fontStyle": "italic"
			}
		},
		{
			"name": "variable",
			"scope": [
				"variable",
				"meta.fstring",
				"source",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#1a86a7",
				"fontStyle": ""
			}
		},
		{
			"name": "boolean",
			"scope": [
				"boolean",
				"constant",
				"markup.inline.raw.string.markdown",
				"markup.underline.link",
			],
			"settings": {
				"foreground": "#7f22c1",
				"fontStyle": "bold"
			}
		},
	]
}
