	"tokenColors": [
		{
			"name": "comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#888",
				"fontStyle": "italic",
			}
		},
		{
			"name": "keyword",
			"scope": [
				"keyword.control",
				"keyword",
				"punctuation.definition.keyword",
				"markup.bold",
			],
			"settings": {
				"foreground": "#b54646",
				"fontStyle": "bold"
			}
		},
		{
			"name": "function",
			"scope": [
				"meta.function-call.generic",
				"meta.function-call",
				"entity.name.function",
				"entity.name.tag",
				"heading"
			],
			"settings": {
				// bb1919 red
				"foreground": "#824aaa",
				"fontStyle": ""
			}
		},
		{
			"name": "operators",
			"scope": [
				"keyword.operator",
				"meta.paragraph",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#ccb20e",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "brackets",
			"scope": [
				"meta.brace",
				"punctuation.section",
				"punctuation.separator.colon",
				"punctuation.separator.parameters",
			],
			"settings": {
				"foreground": "#ccb20e",
				"fontStyle": ""
			}
		},
		{
			"name": "brackets",
			"scope": [
				
				"punctuation.definition",
				"punctuation.definition.tag",
				"constant.character.format.placeholder",
				"punctuation.parenthesis",
				"punctuation.definition.tag.begin.html",
				"source.js-ignored-vscode"
			],
			"settings": {
				"foreground": "#ccb20e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "string",
			"scope": [
				"string",
				"string.quoted",
				"punctuation.definition.string",
				"meta.paragraph"
			],
			"settings": {
				"foreground": "#ccc",
				"fontStyle": "italic"
			}
		},
		{
			"name": "storage",
			"scope": [
				"storage.type.function",
				"entity.other.attribute-name",
				"keyword.other.unit",
				"punctuation.definition.constant.css",
				"markup.italic",
				"support.type",
				"keyword.operator.expression",
				"storage.type.string"
			],
			"settings": {
				"foreground": "#b54646",
				"fontStyle": "italic"
			}
		},
		{
			"name": "variable",
			"scope": [
				"variable",
				"meta.variable",
				"source",
				"support.type.property-name",
				"meta.function-call.arguments"
			],
			"settings": {
				// 15b9eb blue
				"foreground": "#0f7b99",
				"fontStyle": ""
			}
		},
		{
			"name": "boolean",
			"scope": [
				"boolean",
				"constant",
				"markup.inline.raw.string.markdown",
				"markup.underline.link",
			],
			"settings": {
				// 7f22c1 purple
				"foreground": "#ccc",
				"fontStyle": "italic"
			}
		},
	]
}
